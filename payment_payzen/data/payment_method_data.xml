        <field name="code">payzencb</field>
        <field name="primary_payment_method_id" eval="ref('payment_method_payzen')" />
        <field name="sequence">1000</field>
        <field name="active">True</field>
        <field name="image" type="base64" file="payment_payzen/static/src/img/cb.png" />
